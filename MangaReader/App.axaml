<Application xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             x:Class="MangaReader.App"
             xmlns:local="using:MangaReader"
             RequestedThemeVariant="Dark">
    
    <Application.Resources>
        <FontFamily x:Key="RobotoFont">avares://MangaReader/Assets/Fonts/Roboto-Regular.ttf</FontFamily>
    </Application.Resources>
    
    <Application.DataTemplates>
        <local:ViewLocator/>
    </Application.DataTemplates>
  
    <Application.Styles>
        <FluentTheme />
    </Application.Styles>
</Application>